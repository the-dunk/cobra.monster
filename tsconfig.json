{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "paths": {
    "@components/*": [
      "src/components/*"
    ],
    "@assets/*": [
      "src/assets/*"
    ]
  }
}